{
  "expo": {
    "name": "HabitBloom",
    "slug": "HabitBloom",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Asset2.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "imageWidth": 200,
      "resizeMode": "contain",
      "backgroundColor": "#ff90bb",
      "backgroundImage": "../../assets/images/fadhlan.png"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.raylvg.HabitBloom",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.raylvg.HabitBloom"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-splash-screen"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d09148a0-fbe2-43aa-bbd0-f9a3fe6f972b"
      }
    }
  }
}